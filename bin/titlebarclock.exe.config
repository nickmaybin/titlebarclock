<?xml version="1.0" encoding="utf-8" ?>
<configuration>
	<configSections>
		<section name="Timezones" type="TitleBarClock.ApplicationSection, TitleBarClock" />
	</configSections>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
    </startup>
	<appSettings>
		<add key="startLocationTop" value="23" />
		<add key="startLocationLeft" value="1000" />
		<add key="BackColorR" value="31" />
		<add key="BackColorG" value="31" />
		<add key="BackColorB" value="31" />
	</appSettings>	
	<Timezones>
		<Data>
			<add id="1" zone="China Standard Time" format="HH:mm" name="Hong Kong" flag="hong-kong.png" />
			<add id="2" zone="India Standard Time" format="HH:mm" name="Delhi" flag="india.png" />
			<add id="3" zone="GMT Standard Time" format="HH:mm:ss" name="London" flag="united-kingdom.png" />
			<add id="4" zone="Eastern Standard Time" format="HH:mm" name="New York" flag="united-states.png" />
		</Data>
	</Timezones>
</configuration>